    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\OperatingSystem\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~10.2",
        "innmind/url": "~4.0",
        "innmind/ip": "~3.0",
        "vimeo/psalm": "~5.15",
        "innmind/black-box": "~5.5",
        "innmind/coding-standard": "~2.0"
